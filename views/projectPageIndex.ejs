<header>
  <h1>Our Projects</h1>
</header>

<section class="container">
  <% projects.forEach(function(project) { %>
      <div class="card">
          <img src="/<%= project.image%>" alt="<%= project.title %>">
          <div class="card-content">
              <h2><%= project.title %></h2>
              <p><%= project.shortDescription %></p>
              <a target="_blank" href=<%= project.url %>>Read More</a>
          </div>
      </div>
  <% }); %>
</section>

 
