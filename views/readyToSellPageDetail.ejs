  <div id="mediaModal" class="modal">
    <span class="close-modal">&times;</span>
    <div class="modal-content" id="modalContent"></div>
  </div>
  <script>
    // Modal functionality
const modal = document.getElementById('mediaModal');
const modalContent = document.getElementById('modalContent');
const closeBtn = document.getElementsByClassName('close-modal')[0];

function openModal(type, src) {
  modal.style.display = 'block';
  modalContent.innerHTML = '';
  
  // Decode the URI component and ensure proper URL formatting
  const decodedSrc = '/' + decodeURIComponent(src).replace(/^\/+/, '');

  switch(type) {
    case 'image':
      modalContent.innerHTML = `<img src="${decodedSrc}" alt="Full size image">`;
      break;
    case 'video':
      modalContent.innerHTML = `
        <video controls autoplay>
          <source src="${decodedSrc}" type="video/mp4">
          Your browser does not support the video tag.
        </video>`;
      break;
    case 'document':
      const ext = decodedSrc.split('.').pop().toLowerCase();
      if (ext === 'pdf') {
        modalContent.innerHTML = `<iframe src="${decodedSrc}"></iframe>`;
      } else {
        const googleDocsUrl = `https://docs.google.com/viewer?url=${window.location.origin}${decodedSrc}&embedded=true`;
        modalContent.innerHTML = `<iframe src="${googleDocsUrl}"></iframe>`;
      }
      break;
  }
}

// Close modal handlers
closeBtn.onclick = () => {
  modal.style.display = 'none';
  modalContent.innerHTML = '';
}

window.onclick = (event) => {
  if (event.target == modal) {
    modal.style.display = 'none';
    modalContent.innerHTML = '';
  }
}

document.addEventListener('keydown', (event) => {
  if (event.key === 'Escape' && modal.style.display === 'block') {
    modal.style.display = 'none';
    modalContent.innerHTML = '';
  }
});
  </script>
  <section class="blog">
    <img src="/<%= item.thumbnail %>" alt="<%= item.title %>" />
    <div class="blog-body">
      <div class="heading">
        <h3>
          <%= item.title %>
        </h3>
        <span class="date">
          <%= new Date(item.date).toLocaleDateString() %>
        </span>
      </div>
      <p>
        <%= item.shortDescription %>
      </p>

      <!-- Images -->
      <% if (item.images && item.images.length> 0) { %>
        <div class="gallery-section">
          <h4 class="gallery-title">Images</h4>
          <div class="gallery-grid">
            <% item.images.forEach(function(image) { %>
              <div class="gallery-item" onclick="openModal('image', '<%= encodeURI(image) %>')">
                <img src="/<%= image %>" alt="Project Image">
              </div>
            <% }); %>
          </div>
        </div>
        <% } %>

          <!-- Videos -->
          <% if (item.videos && item.videos.length> 0) { %>
            <div class="gallery-section">
              <h4 class="gallery-title">Videos</h4>
              <div class="gallery-grid">
                <% item.videos.forEach(function(video) { %>
                  <div class="gallery-item">
                    <video controls>
                      <source src="/<%= video %>">
                    </video>
                  </div>
                  <% }); %>
              </div>
            </div>
            <% } %>

              <!-- Documents -->
              <% if (item.documents && item.documents.length> 0) { %>
                <div class="gallery-section">
                  <h4 class="gallery-title">Documents</h4>
                  <% item.documents.forEach(function(doc) { %>
                    <div class="document-item">
                      <i class="bi bi-file-earmark-text"></i>
                      <span><%= doc.split('/').pop() %></span>
                      <a href="#" class="preview-btn" onclick="openModal('document', '/<%= doc %>'); return false;">Preview</a>
                      <a href="/<%= doc %>" download class="preview-btn">Download</a>
                    </div>
                    <% }); %>
                </div>
                <% } %>
    </div>
  </section>
