  /* Main Content */
  header {
            /* background: #4a90e2; */
            color: black;
            padding: 20px;
            text-align: center;
            margin-top: 2rem;
        }
  main {
            padding: 20px;
            max-width: 1200px;
            margin: auto;
        }

        .section-title {
            font-size: 2em;
            margin-bottom: 0.5em;
            color: #4a90e2;
            text-align: center;
        }

        /* Selling Benefits Section */
        #benefits {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            margin-bottom: 40px;
        }

        .benefit {
            background: #fff;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            padding: 20px;
            flex: 1 1 300px;
            text-align: center;
            transition: transform 0.3s ease;
        }

        .benefit:hover {
            transform: translateY(-5px);
        }

        .benefit h3 {
            font-size: 1.5em;
            color: #4a90e2;
            margin-bottom: 10px;
        }

        /* Ready to Sell CTA Section */
        #ready-to-sell {
            background: #4a90e2;
            color: white;
            padding: 40px;
            border-radius: 8px;
            text-align: center;
            margin: 40px 0;
        }

        #ready-to-sell p {
            font-size: 1.2em;
            margin-bottom: 20px;
        }

        #ready-to-sell button {
            background: #ffdd57;
            color: #4a90e2;
            border: none;
            padding: 10px 20px;
            font-size: 1.2em;
            cursor: pointer;
            transition: background 0.3s ease;
            border-radius: 4px;
            font-weight: bold;
        }

        #ready-to-sell button:hover {
            background: #ffcc29;
        }

        /* Contact Form */
        #contact {
            background: #fff;
            padding: 30px;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            max-width: 600px;
            margin: auto;
        }

        #contact label {
            display: block;
            font-weight: bold;
            margin-top: 15px;
            color: #4a90e2;
        }

        #contact input, #contact textarea {
            width: 100%;
            padding: 10px;
            margin-top: 5px;
            border: 1px solid #ccc;
            border-radius: 4px;
            margin-bottom: 15px;
            font-size: 1em;
        }

        #contact button {
            background: #4a90e2;
            color: #fff;
            border: none;
            padding: 10px 20px;
            font-size: 1.2em;
            cursor: pointer;
            transition: background 0.3s ease;
            border-radius: 4px;
            font-weight: bold;
        }

        #contact button:hover {
            background: #3b78b4;
        }

        /* Footer */
        footer {
            background: #4a90e2;
            color: #fff;
            text-align: center;
            padding: 15px 0;
            margin-top: 40px;
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            #benefits {
                flex-direction: column;
            }

            header h1 {
                font-size: 2em;
            }

            .section-title {
                font-size: 1.8em;
            }
        }
